  "options": {
    "validateUpsert": true
  },
  "properties": {
    "urlImage": {
      "type": "string",
      "required": true
    },
    "titile": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "createAt": {
      "type": "date",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "users": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "userID",
      "options": {
        "nestRemoting": true
      }
    },
    "typeComment": {
      "type": "belongsTo",
      "model": "TypeComment",
      "foreignKey": "typeCommentID",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
