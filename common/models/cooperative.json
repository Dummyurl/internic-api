  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "urlCoverImage": {
      "type": "string",
      "required": true
    },
    "urlShield": {
      "type": "string",
      "required": true
    },
    "qualification": {
      "type": "string",
      "required": true
    },
